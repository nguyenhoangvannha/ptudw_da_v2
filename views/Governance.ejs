<!DOCTYPE html>
<html lang="en" ng-app="app.Governance">
<head>
  <title>Quản Lý Sản Phẩm</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.0/css/all.css" integrity="sha384-lKuwvrZot6UHsBSfcMvOkWwlCMgc0TaWr+30HWe3a4ltaBwTZhyTEggF5tJv8tbt" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="/assets/css/bootstrap.min.css">
    <link href="/assets/plugins/fontawesome-free-5.0.1/css/fontawesome-all.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="/assets/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="/assets/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="/assets/plugins/OwlCarousel2-2.2.1/animate.css">
    <link rel="stylesheet" type="text/css" href="/assets/plugins/jquery-ui-1.12.1.custom/jquery-ui.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/shop_styles.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/shop_responsive.css">
    <script src="/assets/js/site.js"></script>
    <script src="/assets/js/shop.js"></script>
</head>
<body ng-controller="GovernanceController">


<div class="container">
  <div class="row">
    <div class="col-md-12 col-md-push-2">
      <table class="table table-hover">
        <thead>
          <tr class="bg-success text-center">
            <td style="background-color:darkslategrey" colspan="10"><a href="add">THÊM SẢN PHẨM</a> </td>

          </tr>
          <tr class="bg-primary">
            <th scope="col">ID</th>
            <th scope="col">TÊN</th>
            <th scope="col">HÌNH ẢNH</th>
            <th scope="col">GIÁ</th>
            <th scope="col">LOẠI</th>
            <th scope="col">NHÀ SẢN XUẤT</th>
            <th scope="col">XUẤT XỨ</th>
            <th scope="col">EDIT-DELETE</th>
          </tr>
        </thead>
        <tbody>
        
          <tr ng-repeat="i in [] | range:292">
            <th scope="row">{{products[i].ID}}</th>
            <td>{{products[i].TEN}}</td>
            <td><img src="/assets/carsdata{{products[i].HINHANH}}" alt="" width="50" height="50"></td>
            <td>{{products[i].GIABAN}}</td>
            <td>{{products[i].LOAI}}</td>
            <td>{{products[i].NHASANXUAT}}</td>
            <td>{{products[i].XUATSU}}</td>
            <td><a href="edit/{{products[i].ID}}"><span class="fa fa-edit">EDIT</span></a> - <a href="delete/{{products[i].ID}}"><span class="fa fas fa-trash-alt">DELETE</span></a> </td>
          </tr>
          
        </tbody>
      </table>
    </div>
  </div>
</div>


</body>
<script src="/assets/js/jquery-3.3.1.min.js "></script>
    <script src="/assets/js/popper.js "></script>
    <script src="/assets/js/bootstrap.min.js "></script>
    <script src="/assets/js/angular.min.js "></script>
    <script src="/assets/js/main/Governance.js "></script>
    <script src="/assets/js/main/services/svGovernance.js "></script>
    <script src="/assets/plugins/greensock/TweenMax.min.js "></script>
    <script src="/assets/plugins/greensock/TimelineMax.min.js "></script>
    <script src="/assets/plugins/scrollmagic/ScrollMagic.min.js "></script>
    <script src="/assets/plugins/greensock/animation.gsap.min.js "></script>
    <script src="/assets/plugins/greensock/ScrollToPlugin.min.js "></script>
    <script src="/assets/plugins/OwlCarousel2-2.2.1/owl.carousel.js "></script>
    <script src="/assets/plugins/easing/easing.js "></script>
    <script src="/assets/plugins/Isotope/isotope.pkgd.min.js "></script>
    <script src="/assets/plugins/jquery-ui-1.12.1.custom/jquery-ui.js "></script>
    <script src="/assets/plugins/parallax-js-master/parallax.min.js "></script>
    <script src="/assets/js/shop_custom.js "></script>
</html>