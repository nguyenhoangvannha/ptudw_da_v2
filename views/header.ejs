<header class="header">
	<!-- Top Bar -->
	<div class="top_bar">
		<div class="container">
			<div class="row">
				<div class="col d-flex flex-row">
					<div class="top_bar_contact_item">
						<div class="top_bar_icon">
							<img src="/assets/images/icon/phone.png" alt="">
						</div>+84 168 888 9316</div>
					<div class="top_bar_contact_item">
						<div class="top_bar_icon">
							<img src="/assets/images/icon/mail.png" alt="">
						</div>
						<a href="mailto:fastsales@gmail.com">admin@gmail.com</a>
					</div>
					<div class="top_bar_content ml-auto">
						<div class="top_bar_user">
							<div class="user_icon">
								<img src="/assets/images/icon/user.svg" alt="">
							</div>
							<% if (isLogged)
										{ %>
								<div>
									<%=username%>
								</div>
								<div>
									<a href="signlog/logout">Đăng xuất</a>
								</div>
								<% }
										else
										{ %>
									<div>
										<a href="/signlog">Đăng ký</a>
									</div>
									<div>
										<a href="/signlog">Đăng nhập</a>
									</div>
									<% }
										%>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- Header Main -->
	<div class="header_main">
		<div class="container">
			<div class="row">

				<!-- Logo -->
				<div class="col-lg-2 col-sm-3 col-3 order-1">
					<div class="logo_container">
						<div class="logo">
							<a href="/">Xe hơi FIT</a>
						</div>
					</div>
				</div>

				<!-- Search -->
				<div class="col-lg-6 col-12 order-lg-2 order-3 text-lg-left text-right">
					<div class="header_search">
						<div class="header_search_content">
							<div class="header_search_form_container">
								<form action="search" method="GET" class="header_search_form clearfix">
									<input id='searchKey' name="key" type="text" required="required" class="header_search_input" placeholder="Tìm kiếm sản phẩm...">
									<div class="custom_dropdown">
										<div class="custom_dropdown_list">
											<span class="custom_dropdown_placeholder clc">Tất cả danh mục</span>
											<i class="fas fa-chevron-down"></i>
											<ul id="dropDown01" class="custom_list clc">

												<li onclick="location.href='/search?key=audi';">
													<a class="clc" href="">Audi</a>
												</li>
												<li onclick="location.href='/search?key=bmw';">
													<a class="clc" href="">BMW</a>
												</li>
												<li onclick="location.href='/search?key=chevrolet';">
													<a class="clc" href="#">Chevrolet</a>
												</li>
												<li onclick="location.href='/search?key=ford';">
													<a class="clc" href="#">Ford</a>
												</li>
												<li onclick="location.href='/search?key=lexus';">
													<a class="clc" href="#">Lexus</a>
												</li>
												<li onclick="location.href='/search?key=mercedes-benz';">
													<a class="clc" href="#">Mercedes-Benz</a>
												</li>
												<li onclick="location.href='/search?key=nissan';">
													<a class="clc" href="#">Nissan</a>
												</li>
												<li onclick="location.href='/search?key=toyota';">
													<a class="clc" href="#">Toyota</a>
												</li>
											</ul>
										</div>
									</div>
									<button type="submit" class="header_search_button trans_300" value="Submit">
										<img src="/assets/images/icon/search.png" alt="">
									</button>
								</form>
							</div>
						</div>
					</div>
				</div>

				<!-- Wishlist -->
				<div class="col-lg-4 col-9 order-lg-3 order-2 text-lg-left text-right">
					<div class="wishlist_cart d-flex flex-row align-items-center justify-content-end">
						<!-- Cart -->
						<div class="cart" id="cart-shop">
							<div class="cart_container d-flex flex-row align-items-center justify-content-end">
								<div class="cart_icon">
									<img src="/assets/images/icon/cart.png" alt="">
									<div class="cart_count">
										<span id="count-item" data-count="0">0</span>
									</div>
								</div>
								<div class="cart_content">
									<div class="cart_text">
										<a href="cart.html">Giỏ hàng</a>
									</div>
									<div class="cart_price">$0</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

